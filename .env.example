# This is an example file. Copy this to .env.local and fill in your actual values.
# Do NOT commit your .env.local file to version control.

# Firebase Client-side (public) keys - Get these from Firebase Console > Project Settings > General
NEXT_PUBLIC_FIREBASE_API_KEY="YOUR_FIREBASE_API_KEY_HERE"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID_HERE"
NEXT_PUBLIC_FIREBASE_APP_ID="YOUR_APP_ID_HERE"

# Google Auth Client ID (public) - Get this from Google Cloud Console > APIs & Services > Credentials
NEXT_PUBLIC_GOOGLE_CLIENT_ID="YOUR_GOOGLE_OAUTH_CLIENT_ID_HERE"

# Firebase Server-side (secret) key - Get this from Firebase Console > Project Settings > Service Accounts
# Wrap the entire JSON object in single quotes.
FIREBASE_SERVICE_ACCOUNT_KEY='YOUR_FIREBASE_SERVICE_ACCOUNT_JSON_HERE'

# Google AI (secret) key - Get this from Google AI Studio
GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE"
